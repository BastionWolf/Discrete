#include <iostream>
#include <fstream>
#include <set>
#include <iterator>

using namespace std;

//void universalSetSetter(set <int> U);

int main()
{
	ifstream fin;
	fin.open("sets.dat");

	int i;
	int num;

	set <int> A;			//set A
	set <int> B;			//set B
	set <int> Universal;	//Universal set
	set <int> Union;		//Union set
	set <int> Intersection;	//Intersection

	//universialSetSetter(set <int> U);


	for (i; i < 20; i++)
	{
		Universal.insert(i);
	}

	fin >> num;


}

